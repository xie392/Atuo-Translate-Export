{
  "name": "atuo-translate-export",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:dev":"run-p type-check \"build {@}\" dev--",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "dev": "vite -c vite.popup.config.js",
    "dev:content": "vite -c vite.content.config.js",
    "build": "vite build -c vite.popup.config.js && vite build -c vite.content.config.js && vite build -c vite.background.config.js && node ./build/build.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "chrome": "^0.1.0",
    "element-plus": "^2.4.4",
    "exceljs": "^4.4.0",
    "native-or-bluebird": "1.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "reset-css": "^5.0.2",
    "vite-plugin-chrome-extension": "^0.0.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "xmldom": "0.1.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/chrome": "^0.0.254",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
